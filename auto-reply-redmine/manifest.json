{
  "name": "jQuery DOM",
  "version": "1",
  "manifest_version":2,
  "description": "Manipulate the DOM when the page is done loading",
  "background": "background.html",
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png",
    "default_popup":"popup.html"
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}